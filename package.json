{
  "name": "nwg-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev -p 1111",
    "build": "next build",
    "start": "next start -p 1111",
    "ts-check": "tsc --noemit",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx src pages --quiet",
    "lint-style": "stylelint '**/*.css'",
    "format": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' 'pages/**/*.{ts,tsx}' --write",
    "compile-contract-types": "typechain --target ethers-v5 --out-dir './src/contracts/types' './src/contracts/abi/**/*.json'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.16.1",
    "aos": "^2.3.4",
    "dayjs": "^1.11.6",
    "emoji-picker-react": "^4.4.6",
    "jwt-decode": "^3.1.2",
    "lodash.debounce": "^4.0.8",
    "next": "13.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-keys": "^2.7.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^9.4.1",
    "react-lazyload": "^3.2.0",
    "react-modal": "^3.16.1",
    "react-quill": "^2.0.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.1",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.5.4",
    "stipop-react-sdk": "1.2.6-beta.1",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@babel/runtime": "7.20.1",
    "@types/aos": "^3.0.4",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-lazyload": "^3.2.0",
    "@types/react-modal": "^3.13.1",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.18",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.1",
    "tslib": "^2.4.1",
    "typescript": "4.8.4"
  }
}
